{
    "resource_group": {
        "name": "rg-mymlops",
        "location": "eastus"
    },

    "vm": {
        "name": "vm-mlops",
        "size": "Standard_B1ms",
        "admin_username": "azureuser",
        "ssh_public_key_path": "~/.ssh/id_rsa.pub",
        "virtual_network": "vnet-mlops",
        "sub_network": "subnet-mlops", 
        "network_security_grp": "nsg-mlops",
        "public_ip": "pip-mlops",
        "network_interface": "nic-mlops"
    },

    "databricks": {
        "workspace": "dbw-mymlops",
        "cluster_name": "mymlops-cluster",
        "node_type": "Standard_F4"
    },

    "storage": {
        "storage_account": "storagemymlops",
        "containers": "bronze,silvr,gold"
    },

    "container_registry": {
        "name": "containermymlops",
        "cluster_name": "clustermlops",
        "node": "2",
        "node_size": "Standard_DS2_v2"
    },

    "kubernetes": {
        "k8s_version": "1.32.7"
    },

    "spark": {
        "version": "13.3.x-scala2.12",
        "num_workers":"1"
    }
}